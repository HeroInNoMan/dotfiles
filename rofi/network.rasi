/**
 * Rofi version: 1.5.4
 **/

configuration {
    case-sensitive:    false;
    click-to-exit:     true;
    columns:           1;
    cycle:             true;
    disable-history:   false;
    display-drun:      "apps";
    fake-background:   "screenshot";
    fake-transparency: true;
    fixed-num-lines:   true;
    font:              "mono 16";
    hide-scrollbar:    false;
    lines:             20;
    location:          2;
    separator-style:   "dash";
    show-icons:        true;
    show-match:        true;
    terminal:          "lxterminal";
    width:             60;
}

@import "theme.rasi"

element {
  padding: 5;
  border:  5;
}

element.alternate {
  padding: 5;
  border:  5;
}
